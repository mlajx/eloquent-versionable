{
    "name": "kiqstyle/eloquent-versionable",
    "description": "Versioning behaviour for eloquent models",
    "authors": [
        {
            "name": "Caique Pedro dos Santos",
            "email": "caique_pedro@hotmail.com"
        }
    ],
    "keywords":
        [
            "version",
            "versioning",
            "eloquent",
            "model",
            "laravel",
            "behaviour"
        ],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "illuminate/database": "^8.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "^8.4|^9.3.3",
        "orchestra/testbench": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "Kiqstyle\\EloquentVersionable\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Kiqstyle\\EloquentVersionable\\VersioningServiceProvider"
            ]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kiqstyle\\EloquentVersionable\\Test\\": "tests"
        }
    }
}
